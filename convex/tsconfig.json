        "moduleResolution": "bundler",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": [
        "_generated"
    ]
}